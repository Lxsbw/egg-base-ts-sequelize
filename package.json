{
  "name": "egg-base-ts-sequelize",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start ./dist --daemon --title=egg-server-egg-base-ts-sequelize --ignore-stderr",
    "stop": "egg-scripts stop --title=egg-server-egg-base-ts-sequelize",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "rm -rf dist && ets && tsc -p tsconfig.json && cp -rv package.json dist/",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "del": "rm -rf node_modules && rm -rf yarn.lock"
  },
  "dependencies": {
    "egg": "^2.10.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize-ts": "^0.2.3-0",
    "lodash": "^4.17.20",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "sequelize": "5.21.5",
    "sequelize-typescript": "1.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "sequelize-cli": "5.5.1",
    "tslib": "^1.9.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
